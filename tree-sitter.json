{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "doxygen",
      "camelcase": "Doxygen",
      "title": "Doxygen",
      "scope": "source.doxygen",
      "highlights": "queries/highlights.scm",
      "file-types": [
        "c",
        "h",
        "cpp",
        "cc",
        "cxx",
        "cp",
        "c++",
        "hpp",
        "hh",
        "hxx"
      ],
      "injection-regex": "^doxygen$",
      "class-name": "TreeSitterDoxygen"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Doxygen, JSDoc, etc...",
    "authors": [
      {
        "name": "LAPORTE Cl√©ment",
        "email": "clem@etik.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Clement-Lap/tree-sitter-doxygen"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
