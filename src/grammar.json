{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doxygen",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "doxygen"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "_params_with_briefs": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "param"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": " ["
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_status"
                        }
                      ]
                    },
                    {
                      "type": "STRING",
                      "value": "] "
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "params"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": " ["
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "_status"
                        }
                      ]
                    },
                    {
                      "type": "STRING",
                      "value": "] "
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "return"
        },
        {
          "type": "STRING",
          "value": "returns"
        }
      ]
    },
    "_briefs": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "brief"
        },
        {
          "type": "STRING",
          "value": "file"
        },
        {
          "type": "STRING",
          "value": "author"
        },
        {
          "type": "STRING",
          "value": "authors"
        },
        {
          "type": "STRING",
          "value": "license"
        },
        {
          "type": "STRING",
          "value": "def"
        },
        {
          "type": "STRING",
          "value": "deprecated"
        },
        {
          "type": "STRING",
          "value": "copyright"
        },
        {
          "type": "STRING",
          "value": "date"
        }
      ]
    },
    "_codes_with_briefs": {
      "type": "STRING",
      "value": "fn"
    },
    "_codes_without_brief": {
      "type": "STRING",
      "value": "def"
    },
    "_prefix": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "STRING",
          "value": "\\"
        }
      ]
    },
    "_status": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "in"
        },
        {
          "type": "STRING",
          "value": "out"
        }
      ]
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "code": {
      "type": "PATTERN",
      "value": "[^\\n@_]"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n@]+"
    },
    "doxygen": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SYMBOL",
                "name": "_briefs"
              }
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SYMBOL",
                "name": "_params_with_briefs"
              }
            },
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SYMBOL",
                "name": "_codes_with_briefs"
              }
            },
            {
              "type": "SYMBOL",
              "name": "code"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SYMBOL",
                "name": "_codes_without_brief"
              }
            },
            {
              "type": "SYMBOL",
              "name": "code"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}