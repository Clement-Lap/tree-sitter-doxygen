{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doxygen",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "doxygen"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "with_params": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "param"
        },
        {
          "type": "STRING",
          "value": "param[in]"
        },
        {
          "type": "STRING",
          "value": "param[out]"
        },
        {
          "type": "STRING",
          "value": "params"
        },
        {
          "type": "STRING",
          "value": "params[in]"
        },
        {
          "type": "STRING",
          "value": "params[out]"
        },
        {
          "type": "STRING",
          "value": "return"
        },
        {
          "type": "STRING",
          "value": "returns"
        }
      ]
    },
    "text_only": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "brief"
        },
        {
          "type": "STRING",
          "value": "file"
        },
        {
          "type": "STRING",
          "value": "author"
        },
        {
          "type": "STRING",
          "value": "authors"
        },
        {
          "type": "STRING",
          "value": "license"
        },
        {
          "type": "STRING",
          "value": "def"
        },
        {
          "type": "STRING",
          "value": "deprecated"
        },
        {
          "type": "STRING",
          "value": "copyright"
        },
        {
          "type": "STRING",
          "value": "date"
        },
        {
          "type": "STRING",
          "value": "brief"
        },
        {
          "type": "STRING",
          "value": "fn"
        }
      ]
    },
    "prefix": {
      "type": "STRING",
      "value": "@"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n@]+"
    },
    "doxygen": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "prefix"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "text_only"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "FIELD",
              "name": "identifier",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "prefix"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "with_params"
                  }
                ]
              }
            },
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}