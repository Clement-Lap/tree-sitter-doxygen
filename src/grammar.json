{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doxygen",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "doxygen"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n@]+"
    },
    "_keyword_with_param": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "param"
        },
        {
          "type": "STRING",
          "value": "param[in]"
        },
        {
          "type": "STRING",
          "value": "param[out]"
        },
        {
          "type": "STRING",
          "value": "return"
        }
      ]
    },
    "_status": {
      "type": "CHOICE",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "in"
            },
            {
              "type": "STRING",
              "value": "out"
            }
          ]
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_keyword_without_param": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "brief"
        },
        {
          "type": "STRING",
          "value": "file"
        },
        {
          "type": "STRING",
          "value": "author"
        }
      ]
    },
    "_param": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "_param_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "_keyword_with_param"
        },
        {
          "type": "STRING",
          "value": " {"
        },
        {
          "type": "SYMBOL",
          "name": "_param"
        },
        {
          "type": "STRING",
          "value": "} "
        },
        {
          "type": "PATTERN",
          "value": "[^\\n@ ]*"
        }
      ]
    },
    "_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "_keyword_without_param"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "PATTERN",
          "value": "[^\\n@]*"
        }
      ]
    },
    "identifier_with_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "_keyword_with_param"
        }
      ]
    },
    "identifier_without_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "_keyword_without_param"
        }
      ]
    },
    "variable": {
      "type": "SYMBOL",
      "name": "_param"
    },
    "doxygen": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier_without_param"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier_with_param"
            },
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}